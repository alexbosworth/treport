{
  "name": "treport",
  "description": "Reporters for node-tap",
  "version": "0.0.2",
  "author": "Isaac Z. Schlueter <i@izs.me> (https://izs.me)",
  "dependencies": {
    "cardinal": "^2.1.1",
    "chalk": "^2.4.2",
    "import-jsx": "^2.0.0",
    "ink": "^2.0.0",
    "ms": "^2.1.1",
    "react": "^16.8.2",
    "string-length": "^2.0.0",
    "tap-parser": "^9.0.0",
    "unicode-length": "^2.0.1"
  },
  "devDependencies": {
    "events-to-array": "^1.1.2",
    "ink-testing-library": "^1.0.0",
    "minipass": "^2.3.5",
    "tap": "^13.0.0-rc.9"
  },
  "license": "ISC",
  "scripts": {
    "test": "tap --100",
    "snap": "tap --snapshot --no-coverage",
    "preversion": "npm test",
    "postversion": "npm publish",
    "postpublish": "git push origin --all; git push origin --tags"
  },
  "tap": {
    "test-arg": [
      "--color"
    ]
  },
  "files": [
    "index.js",
    "lib"
  ],
  "nyc": {
    "all": true,
    "include": [
      "index.js",
      "lib/*.js*"
    ]
  }
